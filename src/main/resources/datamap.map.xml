<?xml version="1.0" encoding="utf-8"?>
<data-map xmlns="http://cayenne.apache.org/schema/9/modelMap"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://cayenne.apache.org/schema/9/modelMap http://cayenne.apache.org/schema/9/modelMap.xsd"
	 project-version="9">
	<property name="defaultPackage" value="com.hmg.demo.app.booklibrary.model"/>
	<property name="defaultSchema" value="public"/>
	<db-entity name="book" schema="public">
		<db-attribute name="author" type="VARCHAR" length="32"/>
		<db-attribute name="book_id" type="VARCHAR" length="12"/>
		<db-attribute name="id" type="INTEGER" isPrimaryKey="true" isMandatory="true" length="10"/>
		<db-attribute name="published" type="INTEGER" length="10"/>
		<db-attribute name="publisher" type="VARCHAR" length="32"/>
		<db-attribute name="title" type="VARCHAR" length="32"/>
	</db-entity>
	<obj-entity name="BookP" className="com.hmg.demo.app.booklibrary.model.BookP" dbEntityName="book">
		<obj-attribute name="author" type="java.lang.String" db-attribute-path="author"/>
		<obj-attribute name="bookId" type="java.lang.String" db-attribute-path="book_id"/>
		<obj-attribute name="published" type="int" db-attribute-path="published"/>
		<obj-attribute name="publisher" type="java.lang.String" db-attribute-path="publisher"/>
		<obj-attribute name="title" type="java.lang.String" db-attribute-path="title"/>
	</obj-entity>
</data-map>
